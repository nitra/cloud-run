{
  "name": "nitra-cloud-run",
  "description": "cloud-run helper",
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "demo",
    "npm"
  ],
  "scripts": {
    "d.ts": "cd npm && npx -p typescript tsc src/**/*.js --declaration --allowJs --emitDeclarationOnly --outDir types --skipLibCheck",
    "fix": "npx eslint --fix . && npx prettier --write . "
  },
  "bugs": "https://github.com/nitra/cloud-run/issues",
  "homepage": "https://github.com/nitra/cloud-run",
  "prettier": "@nitra/prettier-config",
  "eslintConfig": {
    "extends": [
      "@nitra"
    ],
    "root": true
  },
  "devDependencies": {
    "@nitra/cspell-dict": "^1.0.118",
    "@nitra/eslint-config": "^1.0.39",
    "@nitra/prettier-config": "^1.0.1"
  },
  "license": "MIT",
  "packageManager": "yarn@4.0.2"
}
